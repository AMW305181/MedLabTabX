<Window x:Class="MedLabTab.Views.OtherViews.ResultInputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Wprowadź wyniki" Height="200" Width="400" WindowStartupLocation="CenterOwner">
    <StackPanel Margin="10">
        <TextBlock Text="Wyniki:" Margin="0,0,0,5"/>
        <TextBox Name="ResultsTextBox" Height="80" TextWrapping="Wrap" AcceptsReturn="True"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,10,0,0">
            <Button Name="OkButton" Width="75" Margin="5" Content="OK" Click="OkButton_Click"/>
            <Button Width="75" Margin="5" Content="Anuluj" Click="CancelButton_Click"/>
        </StackPanel>
    </StackPanel>
</Window>